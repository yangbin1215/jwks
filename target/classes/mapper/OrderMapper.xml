<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yb.mapper.OrderMapper">

    <insert id="addOrder">
        INSERT INTO `order`(odate, uid, bid) VALUES (#{odate}, #{uid}, #{bid});
    </insert>
</mapper>